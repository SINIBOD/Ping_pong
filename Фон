from pygame import *
width = 2000
height = 1200
window = display.set_mode((width, height))
window.fill((134, 217, 209))

clock = time.Clock()
FPS = 144
game = True




while game:
    for e in event.get():
        if e.type == QUIT:
            game = False

            
    display.update()
    clock.tick(FPS)
